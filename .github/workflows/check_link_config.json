{
  "projectBaseUrl":"https://docs.mixpanel.com",
  "ignorePatterns": [
    {
      "pattern": "^http://example.net"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/docs/",
      "replacement": "{{BASEURL}}/docs/"
    },
    {
      "pattern": "^/guides/",
      "replacement": "{{BASEURL}}/guides/"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
