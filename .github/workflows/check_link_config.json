{
  "projectBaseUrl":"https://docs.mixpanel.com",
  "ignorePatterns": [
    {
      "pattern": ".*{PROJECT_ID}.*"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^\/docs\/",
      "replacement": "https:\/\/docs\\.mixpanel\\.com\/docs\/"
    },
    {
      "pattern": "^\/guides\/",
      "replacement": "https:\/\/docs\\.mixpanel\\.com\/guides\/"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
